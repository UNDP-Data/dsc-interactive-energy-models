{
  "template_id": "model",
  "color_scheme": "dark",
  "content": {
    "functionName": "compute_cost002",
    "title": "Clean Cooking Cost Curve",
    "description": "A model exploring costs of cooking options.",
    "latex-functions": [
      "C = F  \\cdot  \\left( \\frac{100}{T} \\right)"
    ],
    "option": {
      "xAxis": {
        "type": "value"
      },
      "yAxis": {
        "type": "value"
      },
      "tooltip": {
        "trigger": "axis"
      },
      "series": []
    },
    "parameters": [
      {
        "symbol": "T",
        "title": "Technology Efficiency",
        "description": "Efficiency of the cookstove",
        "unit": "%",
        "type": "x-axis",
        "min": 10.0,
        "max": 100.0,
        "default": 50.0,
        "interval": 10.0
      },
      {
        "symbol": "F",
        "title": "Fuel Cost",
        "description": "Cost of fuel per kg",
        "unit": "$",
        "type": "fixed"
      }
    ],
    "outputs": [
      {
        "symbol": "C",
        "title": "Total Cost",
        "description": "Total cost per meal",
        "unit": "$",
        "target": 0.2,
        "direction": "below",
        "chart_usage": "y_value"
      }
    ]
  }
}